package chap12_interface.multiflex;

public class SeoulMultiFlexStadium implements MultiFlexStadium {

	@Override
	public void getSportsSchedule(int month) {
		
		System.out.println(month + "월에 예정된 경기는 총 5경기 입니다.");
	}

	@Override
	public int getSportsTicketPrice(int peaple) {
		
		return 7000 * peaple;
	}

	@Override
	public void getSupporterItem(int peaple) {
		
		System.out.println(peaple + "명의 아이템 구매 가격은 " + peaple * 15000 + "원입니다.");
	}

	@Override
	public void printSchedule(int month) {
		
		System.out.println(month + "월에 예정된 콘서트는 총 2건 입니다.");
	}

	@Override
	public int getTicketPrice(int peaple) {
		
		return people * 96000;
	}

	@Override
	public void getRemainSeat() {
		
		
	}

	@Override
	public void playSportsgameOrConcert() {
		
		
	}

}
