package chap07_class;

import chap07_class.game.Numbaseball;

public class _05_PlayNumbaseball {

	public static void main(String[] args) {


		Numbaseball nb = new Numbaseball();
		
		nb.start();
		
		int[] intArr = {1,2,3,4,5};
		for(int a : intArr) {
		    System.out.println(a);
		}
		
		
		
	}

}
