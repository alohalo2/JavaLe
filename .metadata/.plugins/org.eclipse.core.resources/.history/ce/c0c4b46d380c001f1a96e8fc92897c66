package chap14_objectarray.car;

public class _03_ObjectArray {

	public static void main(String[] args) {
		
		// 자바 최상위 클래스인 Object로 배열을 만들면 자바의 모든 타입의 값을 담을 수 있는 배열을 만들 수 있다.
		Object[] objArr = new Object[100];
		
		objArr[0] = new HuyndaiCar("제네시스", 5000, "검정");
		objArr[1] = "aaa";
		objArr[2] = 1000;
		objArr[3] = new int[100];
 		
		// 사용 시에는 변환하는 과장이 필요하다.
		CarInterface carInterface = (charInterface)objArr[0];
		carInterface.carInfo();
		
		String str = (String)objArr[1];
		System.out.println(str);
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
