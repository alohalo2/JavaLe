package chap10_polymorphim.tv;

public class LgTv extends Tv {
	
	@Override
	public void powerOn( ) {
		System.out.println("QLED");
		// super 키워드로 생성자를 호출할 때는 항상 최상단에서 사용해야하지만 인스턴스 메소드를 사용할 때 위치는 중요하지 않다.
		super.powerOn();
	}
	
	@Override
	public void powerOff() {
		System.out.println("QLED");
		super.powerOff();
	}
	
	@Override
	public void operate(int channel) {
		System.out.println("QLED");
		super.operate(channel);
	}
	
//	@Override // private로 되어 있어 메소드 생성 불가능
//	public void channelUp() { 
//		
//	}

}
