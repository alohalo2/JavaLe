package chap12_interface;

import chap12_interface.device.SamsungTv;
import chap12_interface.device.Tv;

public class _02_DefaultMethod {

	public static void main(String[] args) {
		
		Tv tv = new SamsungTv();	
		
		tv.powerOn();
		tv.powerOn();
		// 오버라이드된 default 메소드 사용
		tv.reset();
		
		tv = new LgTv;
		
		
		
	}

}
