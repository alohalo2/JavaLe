package chap99_homework04.parent;

public class Parent {
	
	String surname;
	String skinColor;
	String bloodType;
	int height;
	
	
	public Parent() {
	}
	
	public void surname() {
		System.out.println("성씨는: " + this.surname);
	}
	
	public void skinColor() {
		System.out.println("피부색은: " + this.skinColor);
	}
	
	public void bloodType() {
		System.out.println("성씨는: " + this.bloodType);
	}
	
	public void height() {
		System.out.println("성씨는: " + this.height);
	}
	

}
