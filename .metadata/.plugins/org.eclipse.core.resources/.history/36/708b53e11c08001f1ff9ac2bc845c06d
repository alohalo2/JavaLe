package chap99_homework;

public class SmartPhone {

//	1. SmartPhone 클래스를 생성하고 속성 세 개와 기능 세 개를 자유롭게 만들어보세요.
	
	
	public String company;
	public String color;
	public int price;
	
	public void turnOn() {
		System.out.println("전원을 켜다");
	}
	public void turnOff() {
		System.out.println("전원을 끄다");
	}
	public void phoneCall() {
		System.out.println("전화 통화를 하다");
	}
	
	
}
